{
  "name": "@spencerduballcom/db",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./ddb": {
      "default": "./dist/ddb/index.mjs",
      "types": "./dist/ddb/index.d.ts"
    },
    "./pg": {
      "default": "./dist/pg/index.mjs",
      "types": "./dist/pg/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc --build src && tsx scripts/__dev__/rename-to-mjs.ts",
    "watch": "rm -rf dist/ && tsc -w",
    "db:migrate:create": "sst bind 'tsx scripts/db/migrate-create.ts'",
    "db:migrate": "sst bind 'tsx scripts/db/migrate.ts'",
    "db:reset": "sst bind 'tsx scripts/db/reset.ts'"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "@types/fs-extra": "^11.0.4",
    "arg": "^5.0.2",
    "fs-extra": "^11.1.1",
    "ora": "^7.0.1",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.449.0",
    "@aws-sdk/lib-dynamodb": "^3.449.0",
    "dynamodb-toolbox": "^0.9.2",
    "kysely": "^0.26.3",
    "kysely-postgres-js": "^2.0.0",
    "ms": "3.0.0-canary.1",
    "postgres": "^3.4.3",
    "zod": "^3.22.4"
  }
}
