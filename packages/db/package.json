{
  "name": "@spencerduballcom/db",
  "type": "commonjs",
  "exports": {
    "./ddb": {
      "default": "./dist/ddb/index.js",
      "types": "./dist/ddb/index.d.ts"
    },
    "./pg": {
      "default": "./dist/pg/index.js",
      "types": "./dist/pg/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "ddb": [
        "./dist/ddb/index.d.ts"
      ],
      "pg": [
        "./dist/pg/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc --build src",
    "watch": "rm -rf dist/ && tsc -w",
    "db:migrate": "sst bind \"tsx scripts/db.mts migrate\"",
    "db:migrate:up": "sst bind \"tsx scripts/db.mts migrate:up\"",
    "db:migrate:down": "sst bind \"tsx scripts/db.mts migrate:down\"",
    "db:purge": "sst bind \"tsx scripts/db.mts purge\"",
    "db:clear": "sst bind \"tsx scripts/db.mts clear\"",
    "db:seed": "sst bind \"tsx scripts/db.mts seed\"",
    "db:setup": "sst bind \"tsx scripts/db.mts setup\"",
    "db:reset": "sst bind \"tsx scripts/db.mts reset\"",
    "new-migration": "tsx scripts/new-migration.mts"
  },
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "author": "Spencer Duball",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.7.2",
    "@types/prompts": "^2.4.4",
    "arg": "^5.0.2",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.8",
    "prompts": "^2.4.2",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.332.0",
    "@aws-sdk/lib-dynamodb": "^3.332.0",
    "dynamodb-toolbox": "^0.8.5",
    "kysely": "^0.24.2",
    "ms": "3.0.0-canary.1",
    "pg": "^8.11.0",
    "zod": "^3.21.4"
  }
}
